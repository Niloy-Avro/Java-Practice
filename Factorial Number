import java.util.Scanner;
class A {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the integer: ");
        int a = in.nextInt();
        int b = 1;
        for(int i=1;i<=a;i++) {
            b=b*i;
        }
        System.out.print("The factorial is: "+b);
    }
}
