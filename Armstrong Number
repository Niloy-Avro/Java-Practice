import java.util.Scanner;
class A {
    public static void main (String args[]) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int a=n,b,c=0;
        while(n>0) {
            b=n%10;
            c=c+(b*b*b);
            n=n/10;
        }
        if(c==a) {
            System.out.println(a+" is armstrong num");
        }
        else {
            System.out.println(a+" is not armstrong number");
        }
    }
}
